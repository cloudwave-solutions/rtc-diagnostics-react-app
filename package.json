{
  "name": "cw-rtc-diagnostics-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "@twilio-labs/serverless-api": "^5.7.0",
    "@twilio/rtc-diagnostics": "^1.0.0",
    "@twilio/voice-sdk": "^2.1.1",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "cli-ux": "^6.0.9",
    "dotenv": "^17.2.1",
    "lodash": "^4.17.21",
    "loglevel": "^1.6.8",
    "nanoid": "^5.1.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "5.0.1",
    "twilio": "^5.8.0",
    "typescript": "^5.9.2",
    "util": "^0.12.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{ts,tsx}",
    "test:serverless": "jest -c jest.serverless.config.js",
    "test:ci": "npm run test:ci:app && npm run test:ci:serverless",
    "test:ci:app": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage",
    "test:ci:serverless": "jest -c jest.serverless.config.js --ci --runInBand",
    "serverless:deploy": "npm run build && node serverless/scripts/deploy.js",
    "serverless:remove": "node serverless/scripts/remove.js",
    "serverless:list": "node serverless/scripts/list.js"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-shadow": "off",
      "@typescript-eslint/no-shadow": [
        "warn"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash.set": "^4.3.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "http-proxy-middleware": "^3.0.5",
    "husky": "^9.1.7",
    "jest-junit": "^16.0.0",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^16.1.5",
    "mocha": "^11.7.1",
    "mocha-junit-reporter": "^2.2.1",
    "prettier": "^3.6.2",
    "stdout-stderr": "^0.1.13",
    "superagent": "^10.2.3",
    "ts-jest": "^29.4.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
